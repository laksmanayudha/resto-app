{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/prefer-default-export": "off",
        "no-underscore-dangle": "off",
        "no-undef": "off",
        "no-console": "off",
        "linebreak-style": "off",
        "class-methods-use-this": "off",
        "no-empty-function": "off",
        "import/no-extraneous-dependencies": "off",
        "func-names": "off"
    }
}
